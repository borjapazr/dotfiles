{
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "copyFormatting": false,
  "copyOnSelect": false,
  "keybindings": [],
  "profiles": {
    "defaults": {
      "acrylicOpacity": 0.85,
      "closeOnExit": false,
      "colorScheme": "One Half Dark",
      "cursorHeight": 25,
      "fontFace": "Mars Mono",
      "fontSize": 12,
      "historySize": 9001,
      "padding": "8, 8, 8, 8",
      "snapOnInput": true,
      "startingDirectory": "%USERPROFILE%",
      "useAcrylic": false
    },
    "list": []
  },
  "schemes": [
    {
      "background": "#272935",
      "black": "#21222C",
      "blue": "#BD93F9",
      "brightBlack": "#6272A4",
      "brightBlue": "#D6ACFF",
      "brightCyan": "#A4FFFF",
      "brightGreen": "#69FF94",
      "brightPurple": "#FF92DF",
      "brightRed": "#FF6E6E",
      "brightWhite": "#F8F8F2",
      "brightYellow": "#FFFFA5",
      "cyan": "#8BE9FD",
      "foreground": "#F8F8F2",
      "green": "#50FA7B",
      "name": "Dracula",
      "purple": "#FF79C6",
      "red": "#FF5555",
      "white": "#F8F8F2",
      "yellow": "#FFB86C"
    }
  ]
}